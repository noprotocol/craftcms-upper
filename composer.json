{
  "name": "onetribe/craft-upper",
  "description": "A cache plugin for Craft - supporting multiple Edge Caches",
  "type": "craft-plugin",
  "license": "MIT",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "craft-plugin",
    "cache",
    "cloudflare",
    "fastly",
    "keycdn",
    "akamai"
  ],
  "authors": [
    {
      "name": "Oliver Stark",
      "homepage": "https://www.fortrabbit.com"
    },
    {
      "name": "Job Verplanke",
      "email": "job.verplanke@makesomenoise.nl"
    }
  ],
  "require": {
    "php": "^8.1",
    "craftcms/cms": "^4.0",
    "guzzlehttp/guzzle": "^7.8",
    "akamai-open/edgegrid-auth": "^2.0"
  },
  "require-dev": {
  },
  "autoload": {
    "psr-4": {
      "OneTribe\\Upper\\": "src/"
    }
  },
  "extra": {
    "name": "Upper",
    "handle": "upper",
    "hasCpSettings": false,
    "hasCpSection": false
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  }
}
